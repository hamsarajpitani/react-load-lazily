{
  "name": "react-load-lazily",
  "version": "0.1.20",
  "private": false,
  "main": "dist/lazyload.min.js",
  "type": "module",
  "description": "A tiny library to load anything lazily in react.",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rollup -c -o dist/lazyload.min.js",
    "publish:npm": "rm -rf dist && mkdir dist &&  babel src/component -d dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "load",
    "lazy",
    "react-component"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/preset-react": "^7.18.6",
    "babel-preset-react": "^6.24.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rollup": "^3.10.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "dependencies": {
    "@babel/core": "^7.20.12"
  }
}